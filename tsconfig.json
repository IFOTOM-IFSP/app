{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "typeRoots": [
    "./types",
    "./node_modules/@types"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
  ]
}