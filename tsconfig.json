{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "noEmit": true,
    "skipLibCheck": true,
    "checkJs": false
  },
  "include": [
    "app",
    "src",
    "app.config.*",
    "index.ts",
    "index.tsx"
  ],
  
  "exclude": [
    "node_modules",
    "dist",
    "android",
    "ios",
    "plugins"
  ]
}